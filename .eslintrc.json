{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-console": "off",
    // "no-use-before-define": ["error", { "functions": false }]
    "no-underscore-dangle":  ["error", { "allow": ["_addZero"] }]
    // "no-shadow": "off"
  }
}